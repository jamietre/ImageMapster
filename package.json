    "lint": "grunt lint",
    "lint:fix": "grunt lint:fix",
    "format": "grunt format",
    "format:fix": "grunt format:fix",
    "checklinks": "grunt checklinks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamietre/ImageMapster.git"
  },
  "keywords": [
    "jquery",
    "imagemapster",
    "activate",
